page,
view {
  box-sizing: border-box;
}

page {
  height: 100vh;
  width: 100vw;
}

image {
  width: 100%;
  height: 100%;
}

.gao-page {
  min-height: 100%;
  width: 100%;
  overflow-x: hidden;

  background: linear-gradient(
    180deg,
    rgba(255, 255, 255, 0.882) 0%,
    rgba(255, 255, 255, 0.784) 100%
  );

  backdrop-filter: blur(120px);
}
